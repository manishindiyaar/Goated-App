{
  "name": "goated-app",
  "version": "1.0.0",
  "description": "Privacy-first clinical orchestration platform with Edge AI",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "npm run build:main && concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "npm run build:main && npm run build:vite",
    "build:vite": "vite build",
    "build:main": "tsc -p tsconfig.main.json && tsc -p tsconfig.preload.json",
    "build:electron": "tsc -p tsconfig.main.json && tsc -p tsconfig.preload.json",
    "preview": "vite preview",
    "package": "npm run build && electron-builder",
    "package:mac": "npm run build && electron-builder --mac",
    "package:win": "npm run build && electron-builder --win",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "healthcare",
    "mcp",
    "edge-ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/google": "^3.0.6",
    "@ai-sdk/mcp": "^1.0.5",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "ai": "^6.0.20",
    "electron-squirrel-startup": "^1.0.1",
    "nodejs-whisper": "^0.2.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "rxdb": "^16.21.1",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.16",
    "concurrently": "^9.1.2",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "fast-check": "^4.5.3",
    "jsdom": "^27.4.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^4.0.16",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.goatedapp.app",
    "productName": "GoatedApp",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
